    "import numpy as np \n",
    "lst = [1, 4, 5, 6, 3, 2]\n",
    "\n",
    "# 一次性热编码\n",
    "max_value = max(lst)\n",
    "one_hot_encoded = np.eye(max_value)[np.array(lst)-1]  # 减1以匹配0索引数组\n",
    "\n",
    "print(one_hot_encoded)"
